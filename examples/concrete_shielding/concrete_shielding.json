{
    "Verbosity": {
        "level": 2
    },
    "NeuralNetwork": {
        "Architecture": {
            "model_type": "PNA",
	    "radius": 5.0,
	    "edge_features": ["lengths"],
            "max_neighbours": 110,
            "hidden_dim": 100,
            "num_conv_layers": 4,
            "output_heads": {
                 "node": {
                    "num_headlayers": 3,
                    "dim_headlayers": [1000,1000,1000],
                    "type": "mlp"
                }
            },
            "task_weights": [1.0, 1.0]
        },
        "Variables_of_interest": {
            "input_node_features": [0,1,2,3,4],
            "output_index": [0,1],
            "type": ["node", "node"],
            "denormalize_output": false
        },
        "Training": {
            "num_epoch": 200,
            "batch_size": 12,
            "continue": 0,
            "startfrom": "existing_model",
            "Optimizer": {
                "learning_rate": 1e-3,
		"zero_redundancy": true
            }
        }
    }
}
